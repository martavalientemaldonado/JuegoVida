    }\r\n\r\n        return posicion;\r\n    }\r\n\r\n    public T getPrimero() {\r\n        return list[0];\r\n    }\r\n\r\n    public T getUltimo() {\r\n        return list[getNumElementos()-1];\r\n    }\r\n\r\n    private T getSiguiente(T el) {\r\n        int posicion = getPosicion(el);\r\n        if (posicion < Max) {\r\n            return list[posicion + 1];\r\n        } else {\r\n            return null;\r\n        }\r\n    }\r\n\r\n    public T getElemento(int posicion) {\r\n        if (posicion <= getNumElementos()) {\r\n            return list[posicion];\r\n        } else {\r\n            return null;\r\n        }\r\n    }\r\n}\r\n
